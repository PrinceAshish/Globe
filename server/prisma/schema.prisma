generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(uuid())
  first_name    String
  last_name     String
  email         String   @unique
  mobile_number String   @unique @db.VarChar(10)
  password      String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

